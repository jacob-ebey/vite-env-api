{
	"$schema": "http://json.schemastore.org/tsconfig",
	"extends": "../config/tsconfig.node.json",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"jsx": "react-jsx",
		"lib": ["ES2022", "DOM", "DOM.Iterable"],
		"types": [
			"node",
			"react/canary",
			"react/experimental",
			"react-dom/canary",
			"react-dom/experimental"
		]
	}
}
